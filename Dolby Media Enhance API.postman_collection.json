		{
			"name": "Enhance Audio",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// improve quality: https://docs.dolby.io/media-apis/docs/how-to-improve-audio-by-content-type",
							"",
							"var body = {",
							"    \"input\" : pm.collectionVariables.get('input_file_path'),",
							"    \"output\" : pm.collectionVariables.get('output_file_path'),",
							"    \"content\" : {",
							"        \"type\" : \"studio\"",
							"    },",
							"    \"audio\":{",
							"        \"loudness\":{",
							"            \"enable\":true,",
							"            \"dialog_intelligence\":true",
							"        },",
							"        \"noise\":{",
							"            \"reduction\":{",
							"                \"enable\":true",
							"            }",
							"        },",
							"        \"speech\":{",
							"            \"isolation\": {",
							"                \"enable\": true,",
							"                \"amount\": 75 ",
							"            },",
							"            \"sibilance\": {",
							"                \"reduction\":{",
							"                    \"enable\":true,",
							"                    \"amount\":\"low\"",
							"                }",
							"            },",
							"            \"plosive\": {",
							"                \"reduction\": {",
							"                    \"enable\": true, ",
							"                    \"amount\": \"low\"",
							"                }",
							"            },",
							"            \"click\": {",
							"                \"reduction\": {",
							"                    \"enable\": true, ",
							"                    \"amount\":\"low\"",
							"                }",
							"            }",
							"        }",
							"    }",
							"}",
							"",
							"console.log(body);",
							"var body_str = JSON.stringify(body);",
							"pm.collectionVariables.set('enhance_request_body', body_str);"
						],
